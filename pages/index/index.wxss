/**index.wxss**/
page{
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 275rpx;
  background-color: #fbfbfb;
}
.container{
  width: 690rpx;
  height: auto;
  overflow: hidden;
  margin: auto;
  padding-left: 4rpx;
  padding-right: 4rpx;
  box-sizing: border-box;
}
.imageContainer{
  width: 100%;
  height: 270rpx;
  margin-top: 28rpx;
 }
 .item{
   width: 100%;
   height: 100%;
   border-radius: 16rpx;
   overflow: hidden;
 }
 .item image{
   width: 100%;
   height: 100%;
 }
 .dots-box{
   width: 100%;
   margin-top: 26rpx;
 }
 .dots-active{
   width: 12rpx;
   height: 12rpx;
   border-radius: 50%;
   background: #333333;
 }
 .dots-item{
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background: #999999;
}
/* .tab-bar{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 30rpx;
} */
.tab-bar .bottom_scroll{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
  white-space: nowrap;
}
.tab-item{
  display: inline-block;
  white-space: normal;
  padding-left: 20rpx;
  padding-right: 20rpx;
  font-size: 28rpx;
  color: #333333;
}
.item-active{
  font-size: 38rpx;
  font-weight: bold;
  color: #000000;
}
.active-bar{
  display: block;
  width: 100rpx;
  height: 4rpx;
  background: #4393f4;
  margin-top: 8rpx;
}
/* 头部 */
.head{
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 50rpx;
  box-sizing: border-box;
  padding-top: 6rpx;
  padding-right: 6rpx;
}
.quyu{
  font-size: 48rpx;
  color: #000000;
  font-weight: bold;
}
.tianqi{
  font-size: 32rpx;
  color: #595959;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
}
.tianqi text{
  width: 48rpx;
  height: 28rpx;
  display: block;
  font-size: 24rpx;
  color: #ffffff;
  background: #88A1FC;
  border-radius: 4rpx;
  text-align: center;
  line-height: 28rpx;
  margin-left: 12rpx;
}
.search-box{
  width: 160rpx;
  height: 66rpx;
  background: white;
  border-radius: 60rpx;
  align-self: flex-start;
  box-shadow: 0px 0px 6px 0px rgba(0, 0, 0, 0.17);
}
.search-box image{
  width: 28rpx;
  height: 28rpx;
}
.search-box input{
  width: 60rpx;
  font-size: 26rpx;
  margin-left: 10rpx;
}
.user-bar{
  width: 100%;
  height: 120rpx;
  background: white;
  border-radius: 10rpx;
  box-shadow: 0 2rpx 6rpx rgba(67, 147, 244, .5);
  margin-top: 50rpx;
  box-sizing: border-box;
  padding-right: 24rpx;
}
.user-bar image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin-left: 40rpx;
}
.user-name{
  font-size: 34rpx;
  color: #333333;
  margin-left: 14rpx;
}
.user-stutas{
  font-size: 34rpx;
  color: #4393f4;
}
.page-title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 42rpx;
}
.title-left{
font-size: 38rpx;
font-family: Microsoft YaHei;
font-weight: bold;
color: #333333;
}
.title-right{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.title-right text{
  font-size: 28rpx;
  color: #666666;
  margin-right: 12rpx;
}
.title-right image{
  width: 12rpx;
  height: 20rpx;
}
.navi-list{
  width: 100%;
  height: auto;
  border: 1px #ECECEC solid;
  border-radius: 16rpx;
  margin-top: 60rpx;
  margin-bottom: 40rpx;
}
.list-title{
  width: 100%;
  height: 87rpx;
  border-bottom: 1px solid #ECECEC;
  font-size: 26rpx;
  color: #666666;
  box-sizing: border-box;
  padding-left: 40rpx;
  line-height: 87rpx;
}
.nav-item{
  width: 590rpx;
  margin: 0 auto;
  height: 170rpx;
  border-bottom: 2rpx #ECECEC solid;
}
.nav-item image{
  width: 52rpx;
  height: auto;
}
.list-content{
  flex-direction: column;
  align-items: flex-start;
  margin-left: 30rpx;
}
.list-content text:nth-child(1){
  font-size: 34rpx;
  color: #000000;
  font-weight: bold;
}
.list-content text:nth-child(2){
  font-size: 26rpx;
  color: #ACADA8;
  margin-top: 20rpx;
}
.ssp-btn{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #4393f4;
  box-shadow: 0px 0px 6px 0px rgba(67, 147, 244, .5);
}
.ssp-btn text{
  font-size: 28rpx;
  color: white;
}
.ssp-btn image{
  width: 70rpx;
  height: auto;
}