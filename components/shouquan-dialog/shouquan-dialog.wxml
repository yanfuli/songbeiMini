<view class='dialog' hidden='{{isShow}}'>
  <view class='dialog-box'>
    <view class='dialog-title'>{{title}}</view>
    <view class='dialog-content'>{{content}}</view>
    <button class='dialog-btn' open-type="getUserInfo" lang="zh_CN" bindgetuserinfo="getUserInfo">{{btnOk}}</button>
  </view>
</view>